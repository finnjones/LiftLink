{
    "regions": [
        "syd1"
    ],
    "rewrites": [
        {
            "source": "/auth/(.*)",
            "destination": "/api/src/index.js"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/src/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}